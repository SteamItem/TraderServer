    "lodash": "^4.17.15",
    "mongoose": "^5.9.7",
    "node-telegram-bot-api": "^0.40.0",
    "pm2": "^4.2.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.9",
    "@types/node-telegram-bot-api": "^0.40.2",
    "ts-node": "^8.9.1",
